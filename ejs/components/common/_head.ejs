<meta charset="UTF-8">
<meta http-equiv="content-language" content="ja">
<title><%= status.title %></title>
<meta name="title" content="<%= status.title %>"/>
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1">
<meta name="description" content="<%= status.description %>">
<meta name="keywords" content="<%= status.keywords %>">
<!-- Add OGP Information -->
<meta property="og:type" content="website"/>
<meta property="og:url" content="<%= status.ogpURL %>"/>
<meta property="og:site_name" content="<%= status.ogpSitename %>"/>
<meta property="og:title" content="<%= status.ogpTitle %>"/>
<meta property="og:description" content="<%= status.ogpDescription %>"/>
<meta property="og:image" content="<%= status.ogpImage %>"/>
<meta name="twitter:card" content="summary_large_image"/>
<!-- Add Favicon & WebClip -->
<link rel="apple-touch-icon" sizes="180x180" href="images/favicons/apple-touch-icon.png">
<link rel="icon" type="image/png" href="images/favicons/favicon-32x32.png" sizes="32x32">
<link rel="icon" type="image/png" href="images/favicons/favicon-16x16.png" sizes="16x16">
<link rel="manifest" href="images/favicons/manifest.json">
<link rel="mask-icon" href="images/favicons/safari-pinned-tab.svg" color="">
<meta name="theme-color" content="">